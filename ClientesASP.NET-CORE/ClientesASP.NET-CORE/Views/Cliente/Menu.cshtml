@{
    ViewData["Title"] = "Home";
}
<div class="body-content">
    <div class="jumbotron">
        <h1 class="text-center">Cadastro de pessoas</h1>
    </div>

    <table class="table table-dark">
        <thead>
            <tr>
                <th scope="col">#</th>
                <th scope="col">Nome</th>
                <th scope="col">CPF</th>
                <th scope="col">Data de nascimento</th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <th scope="row">1</th>
                <td>Michel</td>
                <td>000000000</td>
                <td>10/08/1996</td>
            </tr>
            <tr>
                <th scope="row">2</th>
                <td>asasasa</td>
                <td>000000000</td>
                <td>10/08/1996</td>
            </tr>
        </tbody>
    </table>

    <div class="row">
        <div class="col-lg-12">
            <span class="input-group-btn">
                @Html.ActionLink("Nova pessoa", "cadastrar", "cliente",null, new { @class = "btn btn-primary" })
            </span>
        </div>

    </div> <!-- fim row -->
</div>